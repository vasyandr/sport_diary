<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sdairy.simple_workout_daiary">
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".AppContextProgram"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activityes.train_process" />
        <activity android:name=".activityes.train_days_prog" />
        <activity android:name=".activityes.add_exercise" />
        <activity android:name=".activityes.Results" />
        <activity android:name=".activityes.ResultsEdit" />
        <activity android:name=".activityes.PreferencesActivity" />
        <activity android:name=".activityes.ValuesOfBodyActrivity"></activity>
        <activity android:name=".activityes.ValuesBodyEditor"></activity>
    </application>

</manifest>